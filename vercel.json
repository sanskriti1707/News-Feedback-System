{
    "version": 2,
    "builds": [
        { 
            "src": "./index.py", 
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.12",
                "maxDuration": 30
            }
        }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "/" }
    ]
}

  